ALL_LINE=$(links -dump cbr.ru/eng/currency_base/daily/)
echo -n -e "\t\tDate: "
echo "${ALL_LINE}" | grep -G "The Central" | cut -d ' ' -f11
echo "${ALL_LINE}" | grep -E "^\s+Num"
ALL_LINE=$(echo "${ALL_LINE}" | grep -E "^\s+[[:digit:]]{3}")
if [ $# -eq 0 ]
then
echo "${ALL_LINE}"
else
for var in "$@"; do
echo "${ALL_LINE}" | grep -i $var
done
fi
